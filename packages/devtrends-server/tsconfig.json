{
  "compilerOptions": {
    "target": "es2022",
    "module": "ES2022",
    "lib": ["ES2022"],
    "rootDir": "./src",
    "moduleResolution": "node",
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": false,
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
